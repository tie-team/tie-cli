{
  "name": "tie-cli",
  "version": "0.1.3",
  "description": "CLI tool for Tie",
  "author": "forsigner @forsigner",
  "bin": {
    "tie": "./bin/run"
  },
  "eslintConfig": {
    "extends": "tie-app"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80
  },
  "bugs": "https://github.com/tie-team/tie-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.0.1",
    "@types/jest": "^25.2.2",
    "cross-spawn": "^7.0.2",
    "eslint-config-tie-app": "0.0.2",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.1.0",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "ora": "^4.0.4",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.2",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.11",
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^9.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.1",
    "@types/ora": "^3.2.0",
    "@types/yosay": "0.0.29",
    "chai": "^4.2.0",
    "globby": "^11.0.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "standard-version": "^8.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/config",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/templates"
  ],
  "homepage": "https://github.com/tie-team/tie-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "tie",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "tie-team/tie-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "build": "rm -rf lib && tsc -b && oclif-dev manifest",
    "prepare": "npm run build",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "release": "standard-version"
  },
  "types": "lib/index.d.ts"
}
